{
    "expo": {
        "name": "NoteWise",
        "slug": "NoteWise",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "splash": {
            "image": "./assets/ok.png",
            "resizeMode": "cover",
            "backgroundColor": "#f3f3f3"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "package": "com.projekty.noteapp",
            "versionCode": 1,
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#000000"
            }
        },
        "web": {
            "favicon": "./assets/favicon.png"
        }
    }
}
